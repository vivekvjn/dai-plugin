{
  "scopeName": "source.c.extended",
  "injectionSelector": "L:source.c",
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "DAI C",
  "patterns": [
    {
      "include": "#constants"
    },
    {
      "include": "#entityTypes"
    },
    {
      "include": "#storageTypes"
    },
    {
      "include": "#macros"
    }
  ],
  "repository": {
    "constants": {
      "patterns": []
    },
    "entityTypes": {
      "patterns": [
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(BOOLEAN)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(EXTENDED_BOOLEAN)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(REC_NO)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(BACK_REC_NO)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(CHAIN_REC_NO)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(DOUBLE_CHAIN_REC_NO)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(POS_INT)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b(UNSIGNED_INT)\\b"
        },
        {
          "name": "entity.name.type.c.extended",
          "match": "\\b([A-Z0-9_]+_TYPE)\\b"
        }
      ]
    },
    "storageTypes": {
      "patterns": [
        {
          "name": "storage.modifier.c.extended",
          "match": "^\\s*\\b(GLOBAL)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(LOC_NullLocation)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_REC_NO)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(MANY_REC_NO)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_POS_INT)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_TIME)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_DAY)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_TIME_OF_DAY)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_WEIGHT)\\b"
        },
        {
          "name": "storage.type.c.extended",
          "match": "\\b(NULL_VOLUME)\\b"
        }
      ]
    },
    "macros": {
      "patterns": [
        {
          "name": "support.type.c.extended.looper",
          "match": "\\b(LOOP_ALL_[A-Z0-9_]+)\\b"
        }
      ]
    }
  }
}
